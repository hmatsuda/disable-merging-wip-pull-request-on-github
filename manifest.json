{
  "name": "Disable GitHub PR merges",
  "version": "0.4",
  "manifest_version": 2,
  "description": "Disable merging pull requests on GitHub",
  
  "permissions": [
    "tabs", "https://github.com/*/*/pull/*"
  ],
  "content_scripts": [
    {
      "matches": [ "https://github.com/*/*/pull/*" ],
      "js": [ "myscript.js" ]
    }
  ]
}
